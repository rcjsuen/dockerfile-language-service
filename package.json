{
  "name": "dockerfile-language-service",
  "description": "A language service for Dockerfiles to enable the creation of feature-rich Dockerfile editors.",
  "keywords": [
    "language",
    "editor",
    "docker",
    "dockerfile",
    "moby"
  ],
  "version": "0.9.0",
  "author": "Remy Suen",
  "license": "MIT",
  "bugs": "https://github.com/rcjsuen/dockerfile-language-service/",
  "repository": {
    "type": "git",
    "url": "https://github.com/rcjsuen/dockerfile-language-service.git"
  },
  "engines": {
    "node": "*"
  },
  "dependencies": {
    "dockerfile-ast": "0.4.2",
    "dockerfile-utils": "0.10.0",
    "vscode-languageserver-types": "3.17.0-next.3"
  },
  "main": "./lib/main.js",
  "types": "./lib/main.d.ts",
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^6.0.52",
    "mocha": "^9.1.3",
    "nyc": "^15.0.0",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "build": "tsc -p .",
    "prepublish": "tsc -p ./src",
    "watch": "tsc --watch -p .",
    "test": "mocha out/test",
    "nyc": "nyc mocha out/test",
    "nyc-ci": "nyc --reporter=lcov mocha out/test"
  }
}
